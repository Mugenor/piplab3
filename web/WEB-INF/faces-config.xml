<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    metadata-complete="false">
    <managed-bean>
        <managed-bean-name>controller</managed-bean-name>
        <managed-bean-class>Controller</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
        <managed-property>
            <property-name>dao</property-name>
            <value>#{dao}</value>
        </managed-property>
      <managed-property>
        <property-name>point</property-name>
        <value>#{point}</value>
      </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>dao</managed-bean-name>
        <managed-bean-class>DAO</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>
  <managed-bean>
    <managed-bean-name>point</managed-bean-name>
    <managed-bean-class>Point</managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
  </managed-bean>
</faces-config>